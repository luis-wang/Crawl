{
	"name" : "Crawl",
	"description" : "Easy webcrawler chrome extension.",
	"version" : "0.0.1",
	"manifest_version" : 2,

	"icons" : {
		"16" : "icon_16.png",
		"48" : "icon_48.png",
		"128" : "icon_128.png"
	},

	"background" :{
		"scripts" : ["background.js"]
	},

	"browser_action" : {
		"default_popup" : "popup.html",
		"default_icon" : "icon.gif"
	},

	"permissions" : ["notifications"]

}